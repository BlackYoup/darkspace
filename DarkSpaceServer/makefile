PLATFORM=unix

ifneq ("$(windir)","")
PLATFORM=windows
endif

ifneq ("$(WinDir)","")
PLATFORM=windows
endif

ifneq ("$(WINDIR)","")
PLATFORM=windows
endif

UNAME=$(shell uname -o)

ifeq ("$(UNAME)","Cygwin")
PLATFORM=cygwin
endif

ifeq ("$(BUILD_CONFIGURATION)","")
	BUILD_CONFIGURATION=ReleaseLinux
endif

ifeq ("$(BUILD_CONFIGURATION)","DebugLinux")
CC= gcc
CXX= g++
AR= ar
CFLAGS= -g -I ../ -I ../../Medusa/ -D_DEBUG
CXXFLAGS= -g -I ../ -I ../../Medusa/ -D_DEBUG -std=c++0x
ARFLAGS= cru
LDFLAGS= -L ../Bin/ -L ../../Medusa/Bin/ -L ../../Medusa/ThirdParty/LuaJIT/bin/ -lMedusaD -lDarkSpaceD -lWorldD -lNetworkD -lGCQD -lRender3DD -llua51D -lpthread -ldl
RESULT=DarkSpaceServerD
INTERMEDIATE_DIR=DebugLinux/obj
DESTINATION_DIR=../Bin
RM=rm -f
MKDIR=mkdir -p

endif

ifeq ("$(BUILD_CONFIGURATION)","ReleaseLinux")
CC= gcc
CXX= g++
AR= ar
CFLAGS= -g -O2 -I ../ -I ../../Medusa/
CXXFLAGS= -g -O2 -I ../ -I ../../Medusa/ -std=c++0x
ARFLAGS= cru
LDFLAGS= -L ../Bin/ -L ../../Medusa/Bin/ -L ../../Medusa/ThirdParty/LuaJIT/bin/ -lMedusa -lDarkSpace -lWorld -lNetwork -lGCQ -lRender3D -llua51 -lpthread -ldl
RESULT=DarkSpaceServer
INTERMEDIATE_DIR=ReleaseLinux/obj
DESTINATION_DIR=../Bin
RM=rm -f
MKDIR=mkdir -p

endif

SOURCES= DarkSpaceServer.cpp

SOURCEPATHS= DarkSpaceServer.cpp

HEADERS=

VPATH= $(INTERMEDIATE_DIR)

OBJECTNAMES=$(patsubst %.c,%.o,$(patsubst %.cpp,%.o,$(SOURCES)))
OBJECTPATHS=$(addprefix $(INTERMEDIATE_DIR)/,$(OBJECTNAMES))

$(DESTINATION_DIR)/$(RESULT): $(OBJECTNAMES)
ifeq ("$(wildcard $(DESTINATION_DIR) )","")
	-$(MKDIR) $(DESTINATION_DIR)
endif
	$(CXX) $(CXXFLAGS) $(SYSROOTFLAGS) $(OBJECTPATHS) -o $@ $(LDFLAGS)

$(INTERMEDIATE_DIR)/DarkSpaceServer.d: $(SOURCES) $(HEADERS)
ifeq ("$(wildcard $(DESTINATION_DIR) )","")
	-$(MKDIR) $(DESTINATION_DIR)
endif
ifeq ("$(wildcard $(INTERMEDIATE_DIR) )","")
	-$(MKDIR) $(INTERMEDIATE_DIR)
endif
	$(CXX) $(CXXFLAGS) -MM $(SOURCEPATHS) > $@

-include $(INTERMEDIATE_DIR)/DarkSpaceServer.d

clean:
	$(RM) $(DESTINATION_DIR)/$(RESULT)
	$(RM) $(INTERMEDIATE_DIR)/DarkSpaceServer.d
	$(RM) $(OBJECTPATHS)

%.o: %.cpp
	$(CXX) $(CXXFLAGS) -c -o $(INTERMEDIATE_DIR)/$@ $<

%.o: %.c
	$(CC) $(CFLAGS) -c -o $(INTERMEDIATE_DIR)/$@ $<
